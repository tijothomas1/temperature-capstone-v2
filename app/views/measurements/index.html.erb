<h1>Measurements</h1>

<div class="row">
  <% @measurements.each do |measurement| %>
    <div class="col-md-offset-1 col-md-3 col-sm-6 list-view">
      <div class="thumbnail">
        <h2><%= link_to measurement.id, "/measurements/#{measurement.id}" %></h2>
        <h2><%= measurement.reading %></h2> 
      </div>
    </div>
  <% end %>
</div>